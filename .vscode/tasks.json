{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "tsc: build",
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "npm: test",
			"type": "npm",
			"script": "test",
			"problemMatcher": "$tsc",
			"group": "build"
		},
		{
			"label": "wasm: build",
			"type": "shell",
			"command": "${cwd}/build-wasm.sh",
			"group": "build"
		}
	]
}